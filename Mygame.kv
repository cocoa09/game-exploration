FloatLayout:
    Background:
        canvas.before:
            Color:
                rgba: .4,.4,.7,1
            Rectangle:
                size: self.size
                pos: self.pos
                source: ""
            Rectangle:
                size: self.width,138
                pos: self.pos[0],self.pos[1]+self.height-138
                texture: self.floating_ground_texture
